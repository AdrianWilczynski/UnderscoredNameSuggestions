{
    "name": "underscore-name",
    "displayName": "C# Underscored Field Name Suggestions",
    "description": "Type-based underscored field name suggestions for C#",
    "publisher": "adrianwilczynski",
    "repository": {
        "type": "git",
        "url": "https://github.com/AdrianWilczynski/UnderscoredNameSuggestions.git"
    },
    "icon": "img/logo.png",
    "version": "1.1.0",
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "C#",
        "CSharp",
        "Suggestion",
        "Suggestions",
        "Name",
        "Autocompletion",
        "Autocomplete",
        "IntelliSense",
        ".NET",
        ".NET Core"
    ],
    "activationEvents": [
        "onLanguage:csharp"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "chai": "^4.2.0",
        "tslint": "^5.12.1",
        "typescript": "^3.3.1",
        "vscode": "^1.1.28"
    }
}
